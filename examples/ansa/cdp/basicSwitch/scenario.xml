<?xml version="1.0"?>
<scenario>
	<at t="30">
        <disconnect src-module="S1" src-gate="ethg$o[0]" />
        <disconnect src-module="S2" src-gate="ethg$o[0]" />		
  	</at>

  	<at t="70">
        <connect src-module="S1" src-gate="ethg[0]"
                 dest-module="S2" dest-gate="ethg[0]"
                 channel-type="inet.node.ethernet.Eth100M">
        </connect>		
  	</at>
    <at t="100">
        <shutdown module="S1"/>
    </at>
    <at t="290">
        <startup module="S1"/>
    </at>
</scenario>
